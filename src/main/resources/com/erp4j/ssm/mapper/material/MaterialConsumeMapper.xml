  <!-- 查询所有记录 -->
  <select id="find" resultMap="BaseResultMap1">
		SELECT
			consume_id,
			consume_amount,
			consume_date,
			sender,
			receiver,
			mc.note,
			m.material_id,
			w.work_id
		FROM
			material_consume mc
		LEFT JOIN material m
		ON mc.material_id = m.material_id
		LEFT JOIN work w
		ON mc.work_id = w.work_id
	</select>

